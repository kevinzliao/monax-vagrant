#!/bin/zsh
export chain_id="$1" 
export chain_dir="$HOME/.monax/chains/$chain_id"
export addr=$(cat $chain_dir/addresses.csv | grep full_000 | cut -d ',' -f 1)
# Chain emoji: ðŸ”—
